{
  "name": "web-admin-typescript-vue-elementui",
  "version": "1.0.0",
  "description": "基于vue全家桶+typescript+elementui等前端ui框架的web admin项目",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.js",
    "build": "./node_modules/.bin/webpack --config webpack/webpack.prod.js",
    "build-test": "./node_modules/.bin/webpack --config webpack/webpack.prod.js -build-test",
    "upgrade": "./node_modules/.bin/npm-check -u",
    "tsc-init": "./node_modules/.bin/tsc --init",
    "eslint": "./node_modules/.bin/eslint ./ --ext .ts,.js,.vue --fix",
    "husky-init": "husky install",
    "husky-hook": "husky add .husky/pro-commit \"npm run eslint\" && husky add .husky/pro-push \"npm run eslint\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HandsomeTea/web-admin-typescript-vue-elementUI.git"
  },
  "keywords": [
    "webpack",
    "typescript",
    "vue",
    "element-ui",
    "vuex",
    "vue-router"
  ],
  "author": "coco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HandsomeTea/web-admin-typescript-vue-elementUI/issues"
  },
  "homepage": "https://github.com/HandsomeTea/web-admin-typescript-vue-elementUI#readme",
  "browserslist": [
    "> 1%",
    "last 5 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^5.0.2",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "eslint-plugin-vue": "^7.7.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^5.1.3",
    "less": "^4.1.1",
    "less-loader": "^7.1.0",
    "less-plugin-clean-css": "^1.5.1",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "npm-check": "^5.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.0",
    "postcss-preset-env": "^6.7.0",
    "style-resources-loader": "^1.4.1",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "vue-class-component": "^7.2.6",
    "vue-loader": "^15.9.6",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuex-class": "^0.3.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "agentkeepalive": "^4.1.4",
    "axios": "^0.21.1",
    "element-ui": "^2.15.1",
    "vant": "^2.12.10",
    "vue": "^2.6.12",
    "vue-i18n": "^8.24.1",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run eslint"
